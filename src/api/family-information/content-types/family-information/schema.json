    },
    "no_younger_bro": {
      "type": "integer"
    },
    "no_married_bro": {
      "type": "integer"
    },
    "no_elder_sister": {
      "type": "integer"
    },
    "no_younger_sister": {
      "type": "integer"
    },
    "family_origin": {
      "type": "string"
